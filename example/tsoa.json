{
  "entryFile": "example/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["example/controllers/*.ts"],
  "spec": {
    "outputDirectory": "example",
    "specVersion": 3,
    "securityDefinitions": {
      "oauth2": {
        "type": "oauth2",
        "flows": {
          "clientCredentials": {
            "scopes": []
          }
        }
      }
    }
  },
  "routes": {
    "routesDir": "example",
    "authenticationModule": "./example/authentication.ts",
    "esm": true
  }
}
